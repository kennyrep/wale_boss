<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kennyrep.stanbic">

    <application
        android:hardwareAccelerated="false"
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Congratulations">
            <intent-filter>
                <action android:name=".congratulations" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Register">
            <intent-filter>
                <action android:name=".Register" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Homepage"
            android:label="Homepage" />
        <activity android:name=".NewProspect"
            android:label="New Prospect"/>
        <activity android:name=".CallPlan"
            android:label="Call Plan"/>
        <activity android:name=".Visitor"
            android:label="Visitor"/>
        <activity android:name=".VisitorInformation" />
        <activity android:name=".AddNewProspect" />
        <activity android:name=".AjayiVentures"></activity>
    </application>

</manifest>